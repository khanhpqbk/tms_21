<% provide(:title, @course.name) %>
<h1>Course <%= @course.name %></h1>
<p>
<%= @course.name %> |
<%= @course.description %>
</p>

<h2>Subjects in course</h2>
<p>
<%= @course.subjects.all.each do |s| %> 
<%= s.name %> |
<%= s.description %>

<% end %>
</p>

<h2> <%= link_to "Edit", edit_supervisors_course_path(@course) %> <h2>

